#!/bin/bash

rm -rf run
cp -r data run
cd run
ln -fs ../../install/bin/ipe.x .

mpirun -n 10 ./ipe.x

rc=$?
[ $rc -ne 0 ] && echo "Failure code $rc" && exit
[ ! -f IPE_State.apex.201303160005.h5 ] && echo "Output file IPE_State.apex.201303160005.h5 not created." && exit

python ../scripts/compare.py IPE_State.apex.201303160005.h5 ../regression/IPE_State.apex.201303160005.h5.local
